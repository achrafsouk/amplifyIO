{
  "version": 1,
  "framework": { "name": "amplifyIO", "version": "4.18.2" },
  "routes": [
    {
      "path": "/images/*",
      "target": {
        "kind": "ImageOptimization",
        "cacheControl": "public, max-age=3600, immutable"
      }
    },
    {
      "path": "/*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=2"
      }
    },
    {
      "path": "/",
      "target": {
        "kind": "Compute",
        "src": "default"
      }
    }
  ],
  "imageSettings": {
    "sizes": [
      100,
      200,
      1920
    ],
    "domains": [],
    "remotePatterns": [],
    "formats": [],
    "minimumCacheTTL": 60,
    "dangerouslyAllowSVG": false
  }
}
